{
  "version": 1,
  "rules": [
    {
      "domain_keyword": [
        "aria2"
      ],
      "domain_suffix": [
        "smtp"
      ],
      "process_name": [
        "DownloadService.exe",
        "Folx.exe",
        "NetTransport.exe",
        "Thunder.exe",
        "Transmission.exe",
        "WebTorrent Helper.exe",
        "WebTorrent.exe",
        "Weiyun.exe",
        "aria2c.exe",
        "baidunetdisk.exe",
        "fdm.exe",
        "qbittorrent.exe",
        "uTorrent.exe"
      ],
      "domain_regex": [
        "(.+.)?(torrent|announce.php?passkey=|tracker|BitTorrent|bt_key|ed2k|find_node|get_peers|info_hash|magnet:|peer_id=|xunlei)(..+)?",
        "(.+.|^)(360|so|qihoo|360safe|qhimg|360totalsecurity|yunpan).(cn|com)",
        "(.+\\.|^)(360|so)\\.(cn|com)",
        "(.?)(xunlei|sandai|Thunder|XLLiveUD)(.)",
        "(Subject|HELO|SMTP)",
        "(api|ps|sv|offnavi|newvector|ulog.imap|newloc)(.map|).(baidu|n.shifen).com"
      ]
    }
  ]
}